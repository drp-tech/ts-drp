{
	"name": "@ts-drp/object",
	"version": "0.11.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/drp-tech/ts-drp.git"
	},
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"main": "./dist/src/index.js",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"scripts": {
		"benchmark": "tsx tests/hashgraph.bench.ts | tee -a ../benchmark-output.txt && tsx tests/serializer.bench.ts | tee -a ../benchmark-output.txt && tsx tests/memory.bench.ts | tee -a ../benchmark-output.txt",
		"build": "tsc -b tsconfig.build.json",
		"clean": "rm -rf dist/ node_modules/",
		"prepack": "tsc -b",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"watch": "tsc -b -w"
	},
	"devDependencies": {
		"@bufbuild/protobuf": "^2.0.0",
		"@ts-drp/blueprints": "^0.11.0",
		"@ts-drp/keychain": "^0.11.0",
		"@ts-drp/test-utils": "^0.11.0",
		"@ts-drp/utils": "^0.11.0",
		"@types/benchmark": "^2.1.5",
		"benchmark": "^2.1.4",
		"pprof": "^4.0.0",
		"tsx": "4.19.1"
	},
	"dependencies": {
		"@chainsafe/bls": "^8.1.0",
		"@noble/hashes": "^1.7.1",
		"@ts-drp/logger": "^0.11.0",
		"@ts-drp/tracer": "^0.11.0",
		"@ts-drp/types": "^0.11.0",
		"@ts-drp/utils": "^0.11.0",
		"@ts-drp/validation": "^0.11.0",
		"es-toolkit": "1.30.1",
		"fast-deep-equal": "^3.1.3",
		"fast-equals": "^5.2.2",
		"uint8arrays": "^5.1.0"
	}
}
