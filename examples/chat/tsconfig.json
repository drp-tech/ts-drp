{
    "compilerOptions": {
        "target": "es6",
        "module": "ESNEXT",
        "rootDir": ".",
        "strict": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
